{
  "providers": {
    "tmdb": {
      "base_url": "https://api.themoviedb.org/3",
      "api_key": "${TMDB_API_KEY}",
      "default_headers": {
        "Accept": "application/json",
        "User-Agent": "WarpMC/1.0"
      },
      "rate_limits": {
        "per_minute": 40,
        "respect_retry_after": true
      },
      "images": {
        "secure_base_url": "https://image.tmdb.org/t/p",
        "poster_sizes": [
          "w154",
          "w342",
          "w500",
          "w780",
          "original"
        ],
        "backdrop_sizes": [
          "w300",
          "w780",
          "w1280",
          "original"
        ],
        "profile_sizes": [
          "w185",
          "w300",
          "h632",
          "original"
        ]
      },
      "endpoints": {
        "movies": {
          "discover": "/discover/movie",
          "genres": "/genre/movie/list",
          "details": "/movie/{movie_id}",
          "recommendations": "/movie/{movie_id}/recommendations",
          "similar": "/movie/{movie_id}/similar",
          "search": "/search/movie",
          "trending_daily": "/trending/movie/day",
          "popular": "/movie/popular"
        },
        "shows": {
          "discover": "/discover/tv",
          "genres": "/genre/tv/list",
          "details": "/tv/{show_id}",
          "recommendations": "/tv/{show_id}/recommendations",
          "similar": "/tv/{show_id}/similar",
          "search": "/search/tv",
          "trending_daily": "/trending/tv/day",
          "popular": "/tv/popular"
        },
        "people": {
          "details": "/person/{person_id}",
          "combined_credits": "/person/{person_id}/combined_credits"
        },
        "external_ids": "/find/{external_id}",
        "configuration": "/configuration"
      }
    },
    "trakt": {
      "web_url": "https://trakt.tv",
      "base_url": "https://api.trakt.tv",
      "client_id": "${TRAKT_CLIENT_ID}",
      "client_secret": "${TRAKT_CLIENT_SECRET}",
      "default_headers": {
        "Content-Type": "application/json",
        "trakt-api-version": "2",
        "trakt-api-key": "${TRAKT_CLIENT_ID}",
        "User-Agent": "WarpMC/1.0"
      },
      "rate_limits": {
        "per_minute": 60,
        "respect_retry_after": true
      },
      "endpoints": {
        "oauth": {
          "device_code": "/oauth/device/code",
          "poll": "/oauth/device/token",
          "refresh": "/oauth/token"
        },
        "sync": {
          "playback_by_type": "/sync/playback/{type}",
          "history": "/sync/history/{media_type}"
        },
        "movies": {
          "trending": "/movies/trending",
          "popular": "/movies/popular",
          "played": "/movies/played/{period}",
          "watched": "/movies/watched/{period}",
          "collected": "/movies/collected/{period}",
          "anticipated": "/movies/anticipated",
          "related": "/movies/{slug}/related",
          "favorited": "/movies/favorited/{period}"
        },
        "shows": {
          "trending": "/shows/trending",
          "popular": "/shows/popular",
          "played": "/shows/played/{period}",
          "watched": "/shows/watched/{period}",
          "collected": "/shows/collected/{period}",
          "anticipated": "/shows/anticipated",
          "related": "/shows/{slug}/related",
          "progress_watched": "/shows/{show_id}/progress/watched",
          "favorited": "/shows/favorited/{period}"
        },
        "search": {
          "text": "/search"
        },
        "scrobble": {
          "base": "/scrobble/{action}"
        },
        "users": {
          "profile": "/users/{username}",
          "settings": "/users/settings",
          "lists": "/users/{username}/lists",
          "list_items": "/users/{username}/lists/{list_id}/items",
          "list_items_by_type": "/users/{username}/lists/{list_id}/items/{media_type}",
          "lists_by_type": "/users/{username}/lists/{base}"
        }
      }
    },
    "public_domain": {
      "default_headers": {
        "Accept": "application/json",
        "User-Agent": "WarpMC/1.0"
      },
      "sources": {
        "internet_archive_feature_films": {
          "label": "Internet Archive Feature Films",
          "base_url": "https://archive.org",
          "path": "/advancedsearch.php",
          "default_params": {
            "q": "collection:(feature_films) AND mediatype:(movies) AND licenseurl:(creativecommons.org)",
            "output": "json",
            "rows": 200,
            "sort[]": [
              "downloads desc"
            ],
            "fl[]": [
              "identifier",
              "title",
              "creator",
              "description",
              "date",
              "licenseurl",
              "mediatype",
              "downloads"
            ]
          }
        },
        "internet_archive_animation": {
          "label": "Internet Archive Animation",
          "base_url": "https://archive.org",
          "path": "/advancedsearch.php",
          "default_params": {
            "q": "collection:(animationandcartoons) AND mediatype:(movies)",
            "output": "json",
            "rows": 200,
            "sort[]": [
              "downloads desc"
            ],
            "fl[]": [
              "identifier",
              "title",
              "creator",
              "description",
              "date",
              "licenseurl",
              "mediatype",
              "downloads"
            ]
          }
        },
        "internet_archive_classic_tv": {
          "label": "Internet Archive Classic Television",
          "base_url": "https://archive.org",
          "path": "/advancedsearch.php",
          "default_params": {
            "q": "collection:(classic_tv) AND mediatype:(movies)",
            "output": "json",
            "rows": 200,
            "sort[]": [
              "downloads desc"
            ],
            "fl[]": [
              "identifier",
              "title",
              "creator",
              "description",
              "date",
              "licenseurl",
              "mediatype",
              "downloads"
            ]
          }
        },
        "library_of_congress_motion_pictures": {
          "label": "Library of Congress Motion Pictures",
          "base_url": "https://www.loc.gov",
          "path": "/search/",
          "default_params": {
            "fo": "json",
            "q": "public domain film",
            "at": "results",
            "c": 100,
            "fa[]": [
              "original-format:motion picture",
              "online-format:video",
              "rights-advisory:No known restrictions"
            ]
          }
        },
        "library_of_congress_television": {
          "label": "Library of Congress Television",
          "base_url": "https://www.loc.gov",
          "path": "/search/",
          "default_params": {
            "fo": "json",
            "q": "television",
            "at": "results",
            "c": 100,
            "fa[]": [
              "online-format:video",
              "subject:television programs",
              "rights-advisory:No known restrictions"
            ]
          }
        },
        "smithsonian_open_access_videos": {
          "label": "Smithsonian Open Access Videos",
          "base_url": "https://api.si.edu/openaccess/api/v1.0",
          "path": "/search",
          "default_params": {
            "api_key": "${SMITHSONIAN_API_KEY}",
            "q": "online_media_type:Videos AND (content.license:CC0 OR content.license:CCBY)",
            "rows": 100
          }
        },
        "europeana_film_gateway": {
          "label": "Europeana Film Gateway",
          "base_url": "https://api.europeana.eu/record/v2",
          "path": "/search.json",
          "default_params": {
            "wskey": "${EUROPEANA_API_KEY}",
            "query": "TYPE:VIDEO AND (RIGHTS:pd* OR RIGHTS:http*creativecommons*)",
            "media": true,
            "profile": "rich",
            "rows": 100
          }
        },
        "wikimedia_commons_public_domain_video": {
          "label": "Wikimedia Commons Public Domain Video",
          "base_url": "https://commons.wikimedia.org",
          "path": "/w/api.php",
          "default_params": {
            "action": "query",
            "format": "json",
            "generator": "categorymembers",
            "gcmtitle": "Category:Public_domain_films",
            "gcmnamespace": 6,
            "gcmlimit": 50,
            "prop": "imageinfo",
            "iiprop": "url|extmetadata",
            "iiurlwidth": 1280,
            "iiurlheight": 720
          }
        }
      }
    }
  },
  "pipelines": {
    "movies": {
      "primary_provider": "tmdb",
      "supplementary_providers": [
        "trakt"
      ],
      "id_strategies": {
        "tmdb": [
          "tmdb_id",
          "imdb_id"
        ],
        "trakt": [
          "trakt",
          "imdb",
          "tmdb"
        ]
      },
      "public_domain_sources": [
        "public_domain.internet_archive_feature_films",
        "public_domain.internet_archive_animation",
        "public_domain.library_of_congress_motion_pictures",
        "public_domain.smithsonian_open_access_videos",
        "public_domain.europeana_film_gateway"
      ],
      "enable_images": true
    },
    "shows": {
      "primary_provider": "tmdb",
      "supplementary_providers": [
        "trakt"
      ],
      "id_strategies": {
        "tmdb": [
          "tmdb_id"
        ],
        "trakt": [
          "trakt",
          "tvdb",
          "tmdb"
        ]
      },
      "public_domain_sources": [
        "public_domain.internet_archive_classic_tv",
        "public_domain.library_of_congress_television",
        "public_domain.wikimedia_commons_public_domain_video"
      ],
      "enable_images": true
    }
  },
  "content_lists": {
    "public_domain_featured": {
      "label": "Public Domain Feature Films",
      "description": "High demand public domain feature films sourced from multiple open archives.",
      "source": "public_domain.internet_archive_feature_films",
      "max_items": 50
    },
    "public_domain_animation_spotlight": {
      "label": "Classic Animation Spotlight",
      "description": "Curated cartoons and animated shorts in the public domain.",
      "source": "public_domain.internet_archive_animation",
      "max_items": 40
    },
    "public_domain_classic_tv": {
      "label": "Classic Television",
      "description": "Curated classic television episodes and series now in the public domain.",
      "source": "public_domain.internet_archive_classic_tv",
      "max_items": 40
    },
    "public_domain_global_cinema": {
      "label": "Global Public Domain Cinema",
      "description": "European film heritage selections released under open licenses.",
      "source": "public_domain.europeana_film_gateway",
      "max_items": 30
    }
  }
}